    "next": "14.0.3",
    "postcss": "8.4.31",
    "preline": "^2.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
